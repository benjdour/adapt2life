{
  "name": "adapt2life",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NEXT_PRIVATE_TURBOPACK=0 next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "test": "vitest",
    "test:routes": "vitest run tests/app",
    "test:garmin": "vitest run tests/app/api/garmin tests/lib/garmin",
    "test:e2e": "vitest run tests/e2e",
    "typecheck": "tsc --noEmit",
    "audit": "node ./scripts/audit-allowlist.mjs",
    "verify": "npm run lint && npm run typecheck && npm run test && npm run test:routes && npm run audit",
    "db:push": "drizzle-kit push --config drizzle.config.ts",
    "validate:workout": "ts-node --project tsconfig.json --transpile-only scripts/validateWorkout.ts"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.69",
    "@radix-ui/react-slot": "^1.0.4",
    "@stackframe/stack": "^2.8.47",
    "@upstash/redis": "^1.34.7",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "ai": "^5.0.98",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.6",
    "drizzle-orm": "^0.44.7",
    "next": "^16.0.7",
    "pg": "^8.16.3",
    "react": "^19.2.1",
    "react-circular-progressbar": "^2.2.0",
    "react-dom": "^19.2.1",
    "sonner": "^2.0.7",
    "stripe": "^20.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20",
    "@types/pg": "^8.15.6",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.1.0",
    "autoprefixer": "^10.4.20",
    "cross-env": "^10.1.0",
    "eslint": "^9",
    "eslint-config-next": "^16.0.7",
    "jsdom": "^27.0.1",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.13",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "vitest": "^4.0.5"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.52.5"
  }
